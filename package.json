{
  "name": "popup-geojson-map",
  "description": "Put for example, an audio player, in a map popup, using GeoJSON, Leaflet.JS, Lodash...",
  "version": "",
  "author": "Nick Freear <nfreear@yahoo.co.uk>",
  "license": "MIT",
  "repository": "https://github.com/nfreear/audio-popup-map",
  "dependencies": {},
  "devDependencies": {
    "eslint": "^3.8.1",
    "jsonlint-cli": "^1.0.1",
    "node-static": "^0.7.9"
  },
  "files": [
    "src/popup-geojson-map.js",
    ".env-example.js",
    "bin/index.js"
  ],
  "engines": {
    "node": ">= 0.12.0"
  },
  "scripts": {
    "copy-env": "cp -n .env-example.js .env.js",
    "server": "node bin/index.js",
    "envjslint-ci": "node bin/envjslint.js",
    "envjslint": "cat .env.js | pcregrep -M '{\\n.*\\n.*\\n}' | jsonlint-cli",
    "jsonlint": "jsonlint-cli data/*",
    "test": "eslint src/popup-geojson-map.js bin/*.js !.env.js"
  },
  "eslintConfig": {
    "__env": {
      "browser": true
    },
    "globals": {
      "window": false
    },
    "rules": {
      "no-undef": "error",
      "eqeqeq": "error",
      "curly": "error",
      "semi": "error"
    }
  }
}
